model Reservation {
  id              Int      @id @default(autoincrement())
  venue           Venue    @relation(fields: [venueId], references: [id])
  venueId         Int
  user            User     @relation(fields: [userId], references: [id])
  userId          Int
  dateStart       DateTime
  dateEnd         DateTime
  isPendingRating Boolean
  rating          Rating?
}
